{
  "name": "pso-vw-feature-app",
  "version": "0.7.90",
  "buildDate": "2023-06-14",
  "description": "Feature App f√ºr pso empfehlen",
  "author": "Victor Sembelidis <v.sembelidis@pso-vertrieb.de>",
  "scripts": {
    "dev": "vite",
    "dev-vwcb": "cross-env VITE_APP_MANDANT=vwcb vite",
    "dev-vw": "cross-env VITE_APP_MANDANT=volkswagen vite",
    "build": "vue-tsc --noEmit && node ./tools/update-vars.mjs && vite build --mode development",
    "build-testing": "vue-tsc --noEmit && node ./tools/update-vars.mjs && vite build --mode development",
    "build-staging": "vue-tsc --noEmit && node ./tools/update-vars.mjs && vite build --mode staging",
    "build-production": "vue-tsc --noEmit && node ./tools/update-vars.mjs && vite build --mode production",
    "preview": "vite preview --port 5050",
    "test": "cypress open",
    "test:ci": "cypress run",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@feature-hub/dom": "^2.13.0",
    "@vueuse/core": "^7.7.1",
    "@vueuse/integrations": "^7.6.2",
    "bootstrap": "5.1.3",
    "date-fns": "^2.28.0",
    "hashids": "^2.2.10",
    "pinia": "^2.0.11",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.0",
    "@feature-hub/core": "^2.13.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.22",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.9.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.5.5",
    "vite": "^2.7.13",
    "vue-tsc": "^1.0.9"
  }
}
